<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Alarm"
    android:layout_margin="16dp">


    <TextView
        android:id="@+id/textView5"
        android:layout_width="126dp"
        android:layout_height="55dp"
        android:text="@string/new_alarm"
        android:textColor="#121918"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.047" />


    <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
        android:id="@+id/buttontimer"
        android:layout_width="142dp"
        android:layout_height="46dp"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:text="@string/set_time"
        android:textSize="12sp"
        app:backgroundTint="@android:color/white"
        app:icon="@drawable/ic_time"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.181"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.288"
        tools:ignore="MissingConstraints"></com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton>

    <TextView
        android:id="@+id/time"
        style="@style/Base.TextAppearance.AppCompat.Headline"
        android:layout_width="128dp"
        android:layout_height="16dp"
        android:text="@string/waktu"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.058"
        app:layout_constraintStart_toEndOf="@+id/buttontimer"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.299"
        tools:ignore="MissingConstraints">

    </TextView>

    <TextView
        android:id="@+id/write"
        android:layout_width="339dp"
        android:layout_height="34dp"
        android:text="@string/write_your_message_down_below"
        android:textColor="#121918"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.464"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.445"
        tools:ignore="MissingConstraints"></TextView>

    <EditText
        android:id="@+id/alarmname"
        android:layout_width="375dp"
        android:layout_height="57dp"
        android:hint="@string/messagealarm1"
        android:importantForAutofill="no"
        android:inputType="textShortMessage|textLongMessage"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.488"></EditText>

    <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
        android:id="@+id/buttonalarm"
        android:layout_width="173dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:text="@string/set_alarm"
        android:textSize="12sp"
        app:backgroundTint="@android:color/white"
        app:icon="@drawable/ic_alarmadd"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.335"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.769" />

    <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
        android:id="@+id/cancelalarm"
        android:layout_width="173dp"
        android:layout_height="wrap_content"
        android:text="@string/cancel_alarm"
        android:textSize="12sp"
        app:backgroundTint="@android:color/white"
        app:icon="@drawable/ic_alarmoff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.112"
        app:layout_constraintStart_toEndOf="@+id/buttonalarm"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.769"></com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton>

</androidx.constraintlayout.widget.ConstraintLayout>